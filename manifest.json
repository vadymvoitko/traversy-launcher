{
  "manifest_version": 2,
  "name": "Traversy Launcher",
  "description": "Quick launch Traversy Media",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": ["activeTab"],
  "devtools_page": "popup.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
